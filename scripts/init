#!/usr/bin/env sh

cd ~
git clone git@github.com:rajatvig/dotfiles.git ~/dotfiles

cd ~/dotfiles/scripts

sh ./get.brew
sh ./get.git
sh ./get.zsh
sh ./get.fish
sh ./get.spacemacs
sh ./get.spf13-vim
sh ./get.cabal
sh ./get.direnv
sh ./get.npm
sh ./get.apm
sh ./get.osx
sh ./get.python
sh ./get.aspell

cd ~
