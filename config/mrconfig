[DEFAULT]
git_gc = git gc "$@"
git_prune = git remote prune origin "$@"
jobs = 3

[dotfiles]
checkout = git clone 'git@github.com:rajatvig/dotfiles.git' 'dotfiles'

[.emacs.d]
checkout = git clone 'git@github.com:syl20bnr/spacemacs.git' '.emacs.d'

[.config/spacemacs]
checkout = git clone 'git@github.com:rajatvig/spacemacs-config.git' 'spacemacs'

[.config/bash_it]
checkout = git clone 'git@github.com:Bash-it/bash-it.git' 'bash_it'

[.oh-my-zsh]
checkout = git clone 'git@github.com:robbyrussell/oh-my-zsh.git' '.oh-my-zsh'

[.SpaceVim]
checkout = git clone 'https://github.com/SpaceVim/SpaceVim.git' '.SpaceVim'
